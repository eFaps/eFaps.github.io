<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    targetNamespace="http://www.efaps.org/xsd"
    elementFormDefault="qualified"
    xmlns:eFaps="http://www.efaps.org/xsd">

    <!-- RootElements -->
    <xsd:element name="access-set">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_access-set" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="access-type">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_access-type" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="common-systemconfiguration">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_common-systemconfig" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="datamodel-dimension">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_datamodel-dimension" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="datamodel-statusgroup">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_datamodel-statusgroup" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="datamodel-sqltable">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_datamodel-sqltable" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="datamodel-type">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_datamodel-type" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>


    <xsd:element name="data-import">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="definition" type="eFaps:data-import-definition" maxOccurs="unbounded" minOccurs="1" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="data-import-definition">
        <xsd:sequence>
            <xsd:element name="identifier" type="eFaps:data-import-identifier" maxOccurs="1" minOccurs="0" />
            <xsd:element name="type" type="eFaps:data-import-type" maxOccurs="1" minOccurs="1" />
            <xsd:element name="row-listener" type="eFaps:data-import-row-listener" maxOccurs="unbounded" minOccurs="0" />
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required" />
        <xsd:attribute name="file" type="xsd:string" use="required" />
        <xsd:attribute name="keyColumn" type="xsd:string" use="optional" />
        <xsd:attribute name="skipLine" type="xsd:nonNegativeInteger" use="optional" default="0" />
        <xsd:attribute name="execute" type="xsd:boolean" use="optional" default="true" />
        <xsd:attribute name="force" type="xsd:boolean" use="optional" default="false" />
        <xsd:attribute name="accessCheck" type="xsd:boolean" use="optional" default="true" />
        <xsd:attribute name="trigger" type="xsd:boolean" use="optional" default="true" />
        <xsd:attribute name="update" type="xsd:boolean" use="optional" default="false" />
    </xsd:complexType>

    <xsd:complexType name="data-import-identifier">
        <xsd:sequence>
            <xsd:element name="property" type="eFaps:property" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
        <xsd:attribute name="class" type="xsd:string" use="required" />
    </xsd:complexType>

    <xsd:complexType name="data-import-row-listener">
        <xsd:sequence>
            <xsd:element name="property" type="eFaps:property" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
        <xsd:attribute name="class" type="xsd:string" use="required" />
    </xsd:complexType>

    <xsd:complexType name="data-import-type">
        <xsd:sequence>
            <xsd:element name="attribute" type="eFaps:data-import-attribute" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element name="classification" type="eFaps:data-import-classification" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element name="attribute-set" type="eFaps:data-import-attribute-set" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required" />
        <xsd:attribute name="column" type="xsd:string" use="optional" />
        <xsd:attribute name="validate" type="xsd:boolean" use="optional" default="true" />
        <xsd:attribute name="validateClass" type="xsd:string" use="optional" />
    </xsd:complexType>

    <xsd:element name="common-msgphrase">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_msgphrase" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="data-import-attribute-set">
         <xsd:sequence>
            <xsd:element name="property" type="eFaps:property" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required" />
        <xsd:attribute name="class" type="xsd:string" use="optional" />
        <xsd:attribute name="validateClass" type="xsd:string" use="optional" />
    </xsd:complexType>

    <xsd:complexType name="data-import-classification">
        <xsd:sequence>
            <xsd:element name="attribute" type="eFaps:data-import-attribute" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" />
        <xsd:attribute name="column" type="xsd:string" />
        <xsd:attribute name="validate" type="xsd:boolean" default="true" />
        <xsd:attribute name="validateClass" type="xsd:string" />
    </xsd:complexType>

    <xsd:complexType name="data-import-attribute">
        <xsd:sequence>
            <xsd:element name="property" type="eFaps:property" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required" />
        <xsd:attribute name="column" type="xsd:string" use="optional" />
        <xsd:attribute name="class" type="xsd:string" use="optional" />
        <xsd:attribute name="fixedValue" type="xsd:string" use="optional" />
        <xsd:attribute name="validateClass" type="xsd:string" use="optional" />
        <xsd:attribute name="validate" type="xsd:boolean" use="optional" default="true" />
        <xsd:attribute name="typeRegex" type="xsd:string" use="optional" />
        <xsd:attribute name="parentLink" type="xsd:boolean" use="optional" default="false" />
        <xsd:attribute name="overwrite" type="xsd:boolean" use="optional" default="false" />
    </xsd:complexType>

    <xsd:element name="db-store">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_db-store" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="dbproperties">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="bundle" type="eFaps:bundle" maxOccurs="1" minOccurs="1" />
                        <xsd:element name="resource" type="eFaps:propertyResource" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="install">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="application" type="xsd:string" maxOccurs="1" minOccurs="1" />
                <xsd:element name="dependencies" type="eFaps:install_dependencies" maxOccurs="1" minOccurs="0" />
                <xsd:element name="version" type="eFaps:install_version" maxOccurs="unbounded" minOccurs="1" />
                <xsd:element name="files" type="eFaps:install_files" maxOccurs="unbounded" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>This elements are created automatically by the maven goal;
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="integration-webdav">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_integration-webdav" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="jasper-image">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_image" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="bpm-image">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_bpmimage" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>


    <xsd:element name="numbergenerator">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_numbergenerator" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="ui-command">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_ui-command" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="ui-form">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_ui-form" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ui-image">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_image" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="ui-menu">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_ui-menu" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="ui-search">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_ui-search" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="ui-table">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_ui-table" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="user-company">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_user-company" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="user-jaassystem">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_user-jaassystem" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="user-role">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_user-role" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="user-group">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_user-group" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>


    <xsd:element name="help-menu">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_help-menu" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="wiki-image">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="eFaps:AbstractBase">
                    <xsd:sequence>
                        <xsd:element name="definition" type="eFaps:definition_image" maxOccurs="unbounded" minOccurs="1" />
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <!-- Abstract Types -->
    <xsd:complexType name="AbstractBase" abstract="true">
        <xsd:sequence>
            <xsd:element name="uuid" type="xsd:string" maxOccurs="1" minOccurs="1" nillable="false" />
            <xsd:element name="file-application" type="xsd:string" maxOccurs="1" minOccurs="0" />
            <xsd:element name="description" type="xsd:string" maxOccurs="1" minOccurs="0" />
            <xsd:element name="specification" type="xsd:string" maxOccurs="1" minOccurs="0" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AbstractDefinition">
        <xsd:sequence>
            <xsd:element name="version-expression" type="xsd:string" maxOccurs="1" minOccurs="1" />
            <xsd:element name="application-dependencies" type="eFaps:application-dependencies" maxOccurs="1" minOccurs="0" />
            <xsd:element name="profiles" type="eFaps:profileType" maxOccurs="1" minOccurs="0" />
            <xsd:element name="name" type="xsd:string" maxOccurs="1" minOccurs="0" />
            <xsd:element name="parent" type="xsd:string" maxOccurs="1" minOccurs="0" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="application-dependencies">
        <xsd:sequence>
            <xsd:element name="application" maxOccurs="unbounded" minOccurs="1">
                <xsd:complexType>
                    <xsd:attribute name="name" type="xsd:string" use="required" />
                    <xsd:attribute name="exclude" type="xsd:boolean" use="optional" default="false" />
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="profileType">
        <xsd:sequence>
            <xsd:element name="profile" maxOccurs="unbounded" minOccurs="1">
                <xsd:complexType>
                    <xsd:attribute name="name" use="required" />
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <!-- Types -->
    <xsd:complexType name="access">
        <xsd:sequence>
            <xsd:element name="company" type="xsd:string" maxOccurs="unbounded" minOccurs="0" />
            <xsd:element name="group" type="xsd:string" maxOccurs="unbounded" minOccurs="0" />
            <xsd:element name="person" type="xsd:string" maxOccurs="unbounded" minOccurs="0" />
            <xsd:element name="role" type="xsd:string" maxOccurs="unbounded" minOccurs="0" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="accessStatus">
        <xsd:sequence>
            <xsd:element name="key" type="xsd:string" maxOccurs="unbounded" minOccurs="1" />
        </xsd:sequence>
        <xsd:attribute name="group" />
    </xsd:complexType>

    <xsd:complexType name="attribute">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string" maxOccurs="1" minOccurs="1" />
            <xsd:element name="type" maxOccurs="1" minOccurs="1">
                <xsd:simpleType>
                    <xsd:union memberTypes="eFaps:attribute_type_enum"></xsd:union>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="typelink" type="xsd:string" maxOccurs="1" minOccurs="0" />
            <xsd:element name="dimensionUUID" type="xsd:string" maxOccurs="1" minOccurs="0" />
            <xsd:element name="className" type="xsd:string" maxOccurs="1" minOccurs="0" />
            <xsd:element name="sqltable" type="xsd:string" maxOccurs="1" minOccurs="1" />
            <xsd:element name="sqlcolumn" type="xsd:string" maxOccurs="1" minOccurs="1" />
            <xsd:element name="defaultvalue" type="xsd:string" maxOccurs="1" minOccurs="0" />
            <xsd:element name="validate" type="eFaps:validate" maxOccurs="unbounded" minOccurs="0" />
            <xsd:element name="trigger" type="eFaps:trigger4Attribute" maxOccurs="unbounded" minOccurs="0" />
            <xsd:element name="property" type="eFaps:property_attribute" maxOccurs="unbounded" minOccurs="0" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="attribute_type_enum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="AssociationLink" />
            <xsd:enumeration value="BitEnum" />
            <xsd:enumeration value="Boolean" />
            <xsd:enumeration value="Created" />
            <xsd:enumeration value="CreatorLink" />
            <xsd:enumeration value="CompanyLink" />
            <xsd:enumeration value="ConsortiumLink" />
            <xsd:enumeration value="Date" />
            <xsd:enumeration value="DateTime" />
            <xsd:enumeration value="Decimal" />
            <xsd:enumeration value="DecimalWithUoM" />
            <xsd:enumeration value="Enum" />
            <xsd:enumeration value="FormatedString" />
            <xsd:enumeration value="GroupLink" />
            <xsd:enumeration value="Integer" />
            <xsd:enumeration value="IntegerWithUoM" />
            <xsd:enumeration value="Jaxb" />
            <xsd:enumeration value="Link" />
            <xsd:enumeration value="LinkWithRanges" />
            <xsd:enumeration value="Long" />
            <xsd:enumeration value="Modified" />
            <xsd:enumeration value="ModifierLink" />
            <xsd:enumeration value="OID" />
            <xsd:enumeration value="Password" />
            <xsd:enumeration value="PersonLink" />
            <xsd:enumeration value="Rate" />
            <xsd:enumeration value="Real" />
            <xsd:enumeration value="Status" />
            <xsd:enumeration value="String" />
            <xsd:enumeration value="StringWithUoM" />
            <xsd:enumeration value="Time" />
            <xsd:enumeration value="Type" />
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="attributeset">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string" maxOccurs="1" minOccurs="1" />
            <xsd:element name="type" maxOccurs="1" minOccurs="1">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="MultiLineArray" />
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="parent" type="xsd:string" maxOccurs="1" minOccurs="0" />
            <xsd:element name="sqltable" type="xsd:string" maxOccurs="1" minOccurs="1" />
            <xsd:element name="sqlcolumn" type="xsd:string" maxOccurs="1" minOccurs="1" />
            <xsd:element name="uuid" type="xsd:string" maxOccurs="1" minOccurs="1" nillable="false" />
            <xsd:element name="attribute" type="eFaps:attribute" maxOccurs="unbounded" minOccurs="0" />
            <xsd:element name="trigger" type="eFaps:trigger4AttributeSet" maxOccurs="unbounded" minOccurs="0" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="bundle">
        <xsd:attribute name="name" type="xsd:string" />
        <xsd:attribute name="sequence" type="xsd:string" />
        <xsd:attribute name="cacheOnStart" type="xsd:boolean" default="false" use="optional" />
    </xsd:complexType>

    <xsd:complexType name="childs">
        <xsd:sequence>
            <xsd:element name="child" type="xsd:string" maxOccurs="unbounded" minOccurs="1" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="parents">
        <xsd:sequence>
            <xsd:element name="parent" type="xsd:string" maxOccurs="unbounded" minOccurs="1" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="column">
        <xsd:attribute name="name" type="xsd:string" use="required" />
        <xsd:attribute name="type" use="required">
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="BOOLEAN" />
                    <xsd:enumeration value="BLOB" />
                    <xsd:enumeration value="CLOB" />
                    <xsd:enumeration value="DATETIME" />
                    <xsd:enumeration value="DECIMAL" />
                    <xsd:enumeration value="INTEGER" />
                    <xsd:enumeration value="REAL" />
                    <xsd:enumeration value="STRING_LONG" />
                    <xsd:enumeration value="STRING_SHORT" />
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="length" type="xsd:integer" use="optional" />
        <xsd:attribute name="not-null" type="xsd:boolean" use="optional" />
        <xsd:attribute name="scale" type="xsd:integer" use="optional" />
